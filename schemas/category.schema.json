{
  "$id": "category.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Category",
  "type": "object",
  "properties": {
    "id": {
      "description": "The Arxiv category ID",
      "type": "string",
      "minLength": 2,
      "maxLength": 32
    },
    "name": {
      "description": "The Arxiv category full name",
      "type": "string",
      "minLength": 0,
      "maxLength": 256
    }
  },
  "additionalProperties": false,
  "required": ["id", "name"]
}
